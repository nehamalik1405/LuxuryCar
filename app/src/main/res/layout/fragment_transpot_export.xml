<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/margin_10_dp"
    tools:context=".code_files.ui.home.fragment.TranspotExportFragment">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/rootView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        android:paddingBottom="@dimen/margin_20_dp"
        android:visibility="visible">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/linLayoutTop"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="wrap_content"
                android:background="@color/green"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/margin_10_dp"
                    android:fontFamily="@font/muli_semi_bold"
                    android:gravity="center"
                    android:text="@string/str_transport_export"
                    android:textColor="@color/white"
                    android:textSize="@dimen/txt_size_15sp" />

            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerviewTransportExportList"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_15_dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/linLayoutTop" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/btnPayNow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_15_dp"
                android:background="@drawable/drawable_button_background"
                android:backgroundTint="@color/border_color_yellow"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/recyclerviewTransportExportList">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_15_dp"
                    android:layout_marginTop="@dimen/margin_10_dp"
                    android:layout_marginEnd="@dimen/margin_15_dp"
                    android:layout_marginBottom="@dimen/margin_10_dp"
                    android:fontFamily="@font/muli_semi_bold"
                    android:padding="@dimen/margin_0dp"
                    android:text="@string/str_pay_now"
                    android:textAllCaps="true"
                    android:textColor="@color/black"
                    android:textSize="@dimen/margin_14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>


            <LinearLayout
                android:id="@+id/linLayoutmiddle"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_20_dp"
                android:background="@drawable/drawable_gradiant_color"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnPayNow">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/margin_10_dp"
                    android:fontFamily="@font/muli_semi_bold"
                    android:gravity="center"
                    android:text="@string/only_299_for_our_full_service"
                    android:textColor="@color/white"
                    android:textSize="@dimen/txt_size_15sp" />


            </LinearLayout>

            <TextView
                android:id="@+id/txtViewTransportExportDescription"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_30_dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_contact_us_about_your_transport_export_requirements"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_17sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/linLayoutmiddle" />

            <TextView
                android:id="@+id/txtViewDestinationCountry"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_15dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_destination_country"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtViewTransportExportDescription" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/consLayoutDestinationCountry"
                android:layout_width="0dp"
                android:layout_height="@dimen/margin_40dp"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:layout_marginEnd="@dimen/margin_5dp"
                android:background="@drawable/drawable_rounder_edit_text_background"
                android:padding="@dimen/margin_1dp"
                app:layout_constraintEnd_toStartOf="@+id/consLayoutNumberOFCars"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/txtViewDestinationCountry">

                    <Spinner
                        android:id="@+id/spinnerDestinationCountry"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:backgroundTint="@color/gray_600"
                        android:spinnerMode="dialog"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>


                <!-- <ImageView
                     android:id="@+id/imgViewDownDestinationCountry"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginEnd="@dimen/margin_15dp"
                     android:src="@mipmap/ic_down_arrow"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintTop_toTopOf="parent" />-->


            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/txtViewNumberOfCarsToExport"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_15dp"
                android:singleLine="true"
                android:ellipsize="end"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_number_of_cars_to_export"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_14sp"
                app:layout_constraintEnd_toEndOf="@+id/consLayoutNumberOFCars"
                app:layout_constraintBottom_toBottomOf="@+id/txtViewDestinationCountry"
                app:layout_constraintStart_toStartOf="@+id/consLayoutNumberOFCars" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/consLayoutNumberOFCars"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="@dimen/margin_40dp"
                android:layout_marginStart="@dimen/margin_5dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:background="@drawable/drawable_rounder_edit_text_background"
                android:padding="@dimen/margin_1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/consLayoutDestinationCountry"
                app:layout_constraintTop_toBottomOf="@id/txtViewDestinationCountry">



                    <Spinner
                        android:id="@+id/spinnerNumberOFCars"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:backgroundTint="@color/gray_600"
                        android:spinnerMode="dialog" />



                <!--  <ImageView
                      android:id="@+id/imgViewDownNumberOFCars"
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:layout_marginEnd="@dimen/margin_15dp"
                      android:src="@mipmap/ic_down_arrow"
                      app:layout_constraintBottom_toBottomOf="parent"
                      app:layout_constraintEnd_toEndOf="parent"
                      app:layout_constraintTop_toTopOf="parent" />
      -->

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/txtViewCarLinks"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_15dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_car_links"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/consLayoutDestinationCountry" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/consLayoutCarLinks"
                android:layout_width="0dp"
                android:layout_height="@dimen/margin_40dp"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:layout_marginEnd="@dimen/margin_5dp"
                android:background="@drawable/drawable_rounder_edit_text_background"
                android:padding="@dimen/margin_1dp"
                app:layout_constraintEnd_toStartOf="@+id/consLayoutWhereAreYouLooking"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/txtViewCarLinks">



                    <Spinner
                        android:id="@+id/spinnerCarLinks"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:backgroundTint="@color/gray_600"
                        android:spinnerMode="dialog" />




                <!-- <ImageView
                     android:id="@+id/imgViewDownCarLinks"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginEnd="@dimen/margin_15dp"
                     android:src="@mipmap/ic_down_arrow"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintTop_toTopOf="parent" />
     -->

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/txtViewWhereAreYouLooking"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_15dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_number_of_cars_to_export"
                android:singleLine="true"
                android:ellipsize="end"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_14sp"
                app:layout_constraintEnd_toEndOf="@+id/consLayoutWhereAreYouLooking"
                app:layout_constraintBottom_toBottomOf="@+id/txtViewCarLinks"
                app:layout_constraintStart_toStartOf="@+id/consLayoutWhereAreYouLooking" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/consLayoutWhereAreYouLooking"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="@dimen/margin_40dp"
                android:layout_marginStart="@dimen/margin_5dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:background="@drawable/drawable_rounder_edit_text_background"
                android:padding="@dimen/margin_1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/consLayoutCarLinks"
                app:layout_constraintTop_toBottomOf="@id/txtViewCarLinks">


                    <Spinner
                        android:id="@+id/spinnerWhereAreYouLooking"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:backgroundTint="@color/gray_600"
                        android:spinnerMode="dialog" />



                <!-- <ImageView
                     android:id="@+id/imgViewDownWhereAreYouLooking"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_marginEnd="@dimen/margin_15dp"
                     android:src="@mipmap/ic_down_arrow"
                     app:layout_constraintBottom_toBottomOf="parent"
                     app:layout_constraintEnd_toEndOf="parent"
                     app:layout_constraintTop_toTopOf="parent" />-->


            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/txtViewFirstName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_15dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_first_name"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/consLayoutCarLinks" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/consLayoutFirstName"
                android:layout_width="0dp"
                android:layout_height="@dimen/margin_40dp"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:layout_marginEnd="@dimen/margin_5dp"
                android:background="@drawable/drawable_rounder_edit_text_background"
                android:backgroundTint="@color/light_grey"
                android:padding="@dimen/margin_1dp"
                app:layout_constraintEnd_toStartOf="@+id/consLayoutLastName"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/txtViewFirstName">


                <EditText
                    android:layout_width="@dimen/margin_0dp"
                    android:layout_height="@dimen/margin_0dp"
                    android:background="@drawable/drawable_rounder_edit_text_background"
                    android:backgroundTint="@color/white"
                    android:inputType="text"
                    android:maxLines="1"
                    android:paddingStart="@dimen/margin_3dp"
                    android:textSize="@dimen/margin_14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/txtViewLastName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_15dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_last_name"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_14sp"
                app:layout_constraintBottom_toBottomOf="@+id/txtViewFirstName"
                app:layout_constraintStart_toStartOf="@+id/consLayoutLastName" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/consLayoutLastName"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="@dimen/margin_40dp"
                android:layout_marginStart="@dimen/margin_5dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:background="@drawable/drawable_rounder_edit_text_background"
                android:backgroundTint="@color/light_grey"
                android:padding="@dimen/margin_1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/consLayoutFirstName"
                app:layout_constraintTop_toBottomOf="@id/txtViewFirstName">

                <EditText
                    android:layout_width="@dimen/margin_0dp"
                    android:layout_height="@dimen/margin_0dp"
                    android:background="@drawable/drawable_rounder_edit_text_background"
                    android:backgroundTint="@color/white"
                    android:inputType="text"
                    android:maxLines="1"
                    android:paddingStart="@dimen/margin_3dp"
                    android:textSize="@dimen/margin_14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TextView
                android:id="@+id/txtViewEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_15dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_email"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/consLayoutFirstName" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/consLayoutEmail"
                android:layout_width="0dp"
                android:layout_height="@dimen/margin_40dp"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:layout_marginEnd="@dimen/margin_10dp"
                android:background="@drawable/drawable_rounder_edit_text_background"
                android:backgroundTint="@color/light_grey"
                android:padding="@dimen/margin_1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/txtViewEmail">


                <EditText
                    android:layout_width="@dimen/margin_0dp"
                    android:layout_height="@dimen/margin_0dp"
                    android:background="@drawable/drawable_rounder_edit_text_background"
                    android:backgroundTint="@color/white"
                    android:inputType="textEmailAddress"
                    android:maxLines="1"
                    android:paddingStart="@dimen/margin_3dp"
                    android:textSize="@dimen/margin_14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>


            <TextView
                android:id="@+id/txtViewPhone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_15dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_phone"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/consLayoutEmail" />


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/consLayoutPhone"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="@dimen/margin_40dp"
                android:layout_marginStart="@dimen/margin_10dp"
                android:layout_marginTop="@dimen/margin_5dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:background="@drawable/drawable_background_spinner"
                android:backgroundTint="@color/light_grey"
                android:padding="@dimen/margin_1dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/txtViewPhone">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="@dimen/margin_0dp"
                    android:layout_height="@dimen/margin_0dp"
                    android:background="@drawable/drawable_background_spinner"
                    android:backgroundTint="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/consLayoutCityPhoneCode"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:background="@drawable/drawable_button_background"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">


                        <com.hbb20.CountryCodePicker
                            android:id="@+id/ccpCountryCodePicker"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            app:ccp_contentColor="@color/white"
                            app:ccp_showFlag="false"
                            app:ccp_showNameCode="false"
                            app:ccp_textSize="@dimen/margin_13sp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <EditText
                        android:id="@+id/edtTextPhoneNumber"
                        android:layout_width="@dimen/margin_0dp"
                        android:layout_height="@dimen/margin_0dp"
                        android:background="@drawable/drawable_background_spinner"
                        android:backgroundTint="@color/white"
                        android:gravity="center_vertical"
                        android:inputType="phone"
                        android:maxLength="10"
                        android:maxLines="1"
                        android:paddingStart="@dimen/margin_3dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/consLayoutCityPhoneCode"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/btnGetFreeQuote"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_20_dp"
                android:background="@drawable/drawable_button_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/consLayoutPhone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_15_dp"
                    android:layout_marginTop="@dimen/margin_10_dp"
                    android:layout_marginEnd="@dimen/margin_15_dp"
                    android:layout_marginBottom="@dimen/margin_10_dp"
                    android:fontFamily="@font/muli_semi_bold"
                    android:padding="@dimen/margin_0dp"
                    android:text="@string/str_get_free_quote"
                    android:textAllCaps="true"
                    android:textColor="@color/white"
                    android:textSize="@dimen/margin_14sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.cardview.widget.CardView
                android:id="@+id/cardViewCarImage"
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_170_dp"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_20_dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                app:cardCornerRadius="@dimen/margin_5dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnGetFreeQuote">

                <ImageView
                    android:id="@+id/imgViewCar"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_transport_export" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/txtViewTransportExport"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_30_dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                android:fontFamily="@font/muli_semi_bold"
                android:text="@string/str_transport_export_faq_s"
                android:textColor="@color/black"
                android:textSize="@dimen/margin_15_dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardViewCarImage" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerviewTransportExportExpandableList"
                android:layout_width="@dimen/margin_0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/margin_5dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtViewTransportExport" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cardViewBottomCarImage"
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_170_dp"
                android:layout_marginStart="@dimen/margin_15_dp"
                android:layout_marginTop="@dimen/margin_20_dp"
                android:layout_marginEnd="@dimen/margin_15_dp"
                app:cardCornerRadius="@dimen/margin_5dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/recyclerviewTransportExportExpandableList">

                <ImageView
                    android:id="@+id/imgViewBottomCar"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_aeroplane" />
            </androidx.cardview.widget.CardView>


        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:theme="@style/CircularProgress"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>